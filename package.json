{
  "name": "mentorseas-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn && parcel ./src/index.html",
    "build": "yarn && parcel build ./src/index.html -d build/",
    "test": "yarn && yarn lint && yarn flow",
    "transfer": "scp -r build/* simonz@lnxsrv09.seas.ucla.edu:/usr/public/www/mentorship",
    "deploy": "yarn build && yarn transfer",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "flow": "flow status",
    "prepush": "yarn test"
  },
  "dependencies": {
    "app": "^0.1.0",
    "autoprefixer": "7.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "components": "^0.1.0",
    "dotenv": "4.0.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "images": "^3.0.2",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "pages": "^0.0.16",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "radium": "^0.19.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "styled-components": "^3.2.6",
    "styles": "^0.2.1",
    "url-loader": "0.5.9",
    "views": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.12.1",
    "sass": "^1.19.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "src/pages/StaffPage/StaffPage.js",
    "src/components/StaffMember"
  ],
  "homepage": "http://mentorship.seas.ucla.edu",
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
